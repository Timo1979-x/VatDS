	<queryString>
		<![CDATA[select count(dk.id_dso_diagn_card) as "total", 
(select count(dk2.id_dso_diagn_card)
        from
            dso_diagn_card dk2
        where
            dk2.date_ot between $P{startMonth} and $P{endMonth} and
            dk2.Status_Doc=1) as "good",
            (select count(dk2.id_dso_diagn_card)
        from
            dso_diagn_card dk2
        where
            dk2.date_ot between $P{startMonth} and $P{endMonth} and
            dk2.Status_Doc=3) as "good2"
from dso_diagn_card dk
WHERE
     dk.`date_card` BETWEEN $P{startMonth} and $P{endMonth} and 
     dk.`Status_Doc` is not null]]>
	</queryString>
